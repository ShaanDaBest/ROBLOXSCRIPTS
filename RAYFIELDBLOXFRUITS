local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "AEROHUB",
   Icon = 0, -- Icon in Topbar. Can use Lucide Icons (string) or Roblox Image (number). 0 to use no icon (default).
   LoadingTitle = "Loading",
   LoadingSubtitle = "by Dev58",
   ShowText = "Dev58", -- for mobile users to unhide rayfield, change if you'd like
   Theme = "Default", 

   ToggleUIKeybind = "K"

   DisableRayfieldPrompts = false,
   DisableBuildWarnings = false, -- Prevents Rayfield from warning when the script has a version mismatch with the interface

   ConfigurationSaving = {
      Enabled = false,
      FolderName = BloxFruits58, -- Create a custom folder for your hub/game
      FileName = "Sea 1"
   },

   Discord = {
      Enabled = false, 
      Invite = "noinvitelink"
      RememberJoins = true 
   },

   KeySystem = true,
   KeySettings = {
      Title = "Sea1Key",
      Subtitle = "Key System For Sea 1 Players",
      Note = "No method of obtaining the key is provided", 
      FileName = "Sea1Key58", -- It is recommended to use something unique as other scripts using Rayfield may overwrite your key file
      SaveKey = false, 
      GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
      Key = {"Dev58"} 
   }
})
